{
    "name": "Senko",
    "description": "Senko Welcome Bot",
    "repository": "https://github.com/MoeZilla/senko-telegram-bot",
    "env": {
        "BOT_TOKEN": {
            "description": "telegram bot token here",
            "required": true
        },
        "REMOVEBG_API": {
            "description": "telegram api id here",
            "required": true
        },
        "UNSCREEN_API": {
            "description": "telegram api hash here",
            "required": true
        },
        "MONGO_URL": {
            "description": "mongo db url",
            "required": true
        }
    },
    "addons": [
        {
            "plan": "heroku-postgresql"
        }
    ]
}
